    "src/config.ts",
    "src/model/HadoopData.ts",
    "src/parser/main.ts",
    "src/socket/main.ts",
    "scripts/_references.d.ts",
    "scripts/typings/async/async.d.ts",
    "scripts/typings/node/node.d.ts",
    "scripts/typings/socket.io-client/socket.io-client.d.ts"
  ]
}